{
  "base_url_template": "https://{subdomain}.atlan.com",
  "subdomain_auth_token_map": {
    "demo": "test_token_demo",
    "staging": "test_token_staging"
  },
  "connections_api": {
    "url": "/api/meta/search/indexsearch",
    "payload": {
      "dsl": {
        "size": 50,
        "sort": [
          {
            "__timestamp.date": {
              "format": "epoch_second",
              "order": "desc"
            }
          }
        ],
        "query": {
          "bool": {
            "filter": {
              "bool": {
                "must": [
                  {
                    "term": {
                      "__state": "ACTIVE"
                    }
                  },
                  {
                    "term": {
                      "__typeName.keyword": "Connection"
                    }
                  }
                ]
              }
            }
          }
        }
      },
      "attributes": [
        "connectorName",
        "isPartial",
        "name",
        "qualifiedName"
      ],
      "suppressLogs": true
    }
  },
  "api_map": {
    "databricks": "databases_api",
    "snowflake": "databases_api",
    "oracle": "databases_api",
    "postgres": "databases_api",
    "mysql": "databases_api",
    "tableau": "tableau_api",
    "powerbi": "powerbi_api"
  },
  "databases_api": {
    "url": "/api/meta/search/indexsearch",
    "payload": {
      "dsl": {
        "sort": [
          {
            "name.keyword": {
              "order": "asc"
            }
          }
        ],
        "size": 100,
        "query": {
          "bool": {
            "filter": {
              "bool": {
                "must": [
                  {
                    "term": {
                      "__state": "ACTIVE"
                    }
                  },
                  {
                    "term": {
                      "__typeName.keyword": "Database"
                    }
                  },
                  {
                    "term": {
                      "connectionQualifiedName": "PLACEHOLDER_TO_BE_REPLACED"
                    }
                  }
                ]
              }
            }
          }
        }
      },
      "attributes": [
        "name",
        "qualifiedName",
        "connectionQualifiedName"
      ],
      "suppressLogs": true
    }
  },
  "tableau_api": {
    "url": "/api/meta/search/indexsearch",
    "payload": {
      "dsl": {
        "sort": [
          {
            "name.keyword": {
              "order": "asc"
            }
          }
        ],
        "size": 100,
        "query": {
          "bool": {
            "filter": {
              "bool": {
                "must": [
                  {
                    "term": {
                      "__state": "ACTIVE"
                    }
                  },
                  {
                    "bool": {
                      "should": [
                        {
                          "term": {
                            "__typeName.keyword": "TableauDatasource"
                          }
                        },
                        {
                          "term": {
                            "__typeName.keyword": "TableauWorkbook"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "term": {
                      "connectionQualifiedName": "PLACEHOLDER_TO_BE_REPLACED"
                    }
                  }
                ]
              }
            }
          }
        }
      },
      "attributes": [
        "name",
        "qualifiedName",
        "connectionQualifiedName"
      ],
      "suppressLogs": true
    }
  },
  "powerbi_api": {
    "url": "/api/meta/search/indexsearch",
    "payload": {
      "dsl": {
        "sort": [
          {
            "name.keyword": {
              "order": "asc"
            }
          }
        ],
        "size": 100,
        "query": {
          "bool": {
            "filter": {
              "bool": {
                "must": [
                  {
                    "term": {
                      "__state": "ACTIVE"
                    }
                  },
                  {
                    "bool": {
                      "should": [
                        {
                          "term": {
                            "__typeName.keyword": "PowerBIDataset"
                          }
                        },
                        {
                          "term": {
                            "__typeName.keyword": "PowerBIReport"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "term": {
                      "connectionQualifiedName": "PLACEHOLDER_TO_BE_REPLACED"
                    }
                  }
                ]
              }
            }
          }
        }
      },
      "attributes": [
        "name",
        "qualifiedName",
        "connectionQualifiedName"
      ],
      "suppressLogs": true
    }
  }
}